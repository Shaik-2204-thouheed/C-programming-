#include <stdio.h>

int main() {
    int n, digit;

    printf("Enter a number: ");
    scanf("%d", &n);

    printf("Individual digits:\n");

    while (n > 0) {
        digit = n % 10;     
        printf("%d\n", digit);
        n = n / 10;         
    }

    return 0;
}.   
       //output 
7
6
5
4
